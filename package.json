{
  "name": "start-with-managed",
  "version": "1.0.0",
  "description": "This project start from expo managed flow to development build",
  "scripts": {
    "start": "npx expo start --dev-client",
    "android": "expo start --android",
    "test": "jest",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android:preview": "npx eas-cli build --profile preview --platform android --local",
    "build:android:dev": "npx eas-cli build --profile development --platform android"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  },
  "dependencies": {
    "@expo/config-plugins": "~8.0.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "@realm/react": "^0.10.0",
    "expo": "^51.0.31",
    "expo-asset": "~10.0.10",
    "expo-barcode-scanner": "~13.0.1",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.25",
    "expo-image-picker": "~15.0.7",
    "expo-status-bar": "~1.12.1",
    "expo-updates": "~0.25.24",
    "expo-web-browser": "~13.0.3",
    "i18next": "^23.14.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^12.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-get-random-values": "~1.11.0",
    "react-native-paper": "^5.2.0",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-use-sound": "^1.2.2",
    "realm": "^12.13.1",
    "styled-components": "^6.1.13",
    "expo-camera": "~15.0.14",
    "expo-file-system": "~17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.1",
    "@types/jest": "^29.5.5",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-native": "^4.0.0",
    "globals": "^15.9.0",
    "jest-expo": "~51.0.4",
    "typescript": "~5.3.3"
  },
  "private": true
}
